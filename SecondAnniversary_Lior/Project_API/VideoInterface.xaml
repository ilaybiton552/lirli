<UserControl x:Class="Project_API.VideoInterface"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Project_API"
             mc:Ignorable="d" 
             d:DesignHeight="75" d:DesignWidth="825">
    <Grid x:Name="grid">
        <Image Source="/images/play.png" HorizontalAlignment="Left" Width="50" Height="50" MouseLeftButtonDown="Image_MouseLeftButtonDown"/>
        <Line x:Name="still" HorizontalAlignment="Left" Y1="37.5" Y2="37.5" X1="80" X2="620" Stroke="DarkGray" StrokeThickness="50" MouseLeftButtonDown="Line_MouseLeftButtonDown"/>
        <Line x:Name="progLine" HorizontalAlignment="Left" Y1="37.5" Y2="37.5" X1="80" X2="80" Stroke="Gray" StrokeThickness="0" MouseLeftButtonDown="Line_MouseLeftButtonDown"/>
    </Grid>
</UserControl>
